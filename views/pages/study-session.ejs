<!DOCTYPE html>
<html>
  <%- include('../partials/head'); %>

  <body>
    <%- include('../partials/header');%>

    <section id="study">
      <div id="study-list">
        <div class="todo-list-container">
          <div class="list-header">
            <h4 class="list-name" contenteditable>Target List</h4>
            <div class="modal">
              <h4><span>List Actions</span></h4>
              <p class="delete-list">Delete This List..</p>
            </div>
          </div>

          <div class="items">
            <% for (let i = 0; i < items.length; i++) { %>
            <div
              id="card-<%= items[i].itemID %>"
              class="todo-card"
              draggable="true"
            >
              <div class="todo-content">
                <input type="checkbox" />
                <p class="todo-description"><%= items[i].itemName %></p>
                <br />
                <div class="flaticons">
                  <img
                    class="edit"
                    src="data:image/svg+xml;base64,"
                  />
                  <img
                    class="trash"
                    src="data:image/svg+xml;base64,"
                  />
                </div>
              </div>
            </div>
            <% } %>
          </div>
        </div>
      </div>
      <div id="study-timer"></div>
    </section>
    <script
      type="module"
      src="../../../javascript/study-session.js"
      async
      defer
    ></script>
  </body>
</html>
